<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorBackgroundDefault"
        android:orientation="vertical">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout3"
            style="@style/CardStyle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">


            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/user_name_title_shimmer"
                android:layout_width="100dp"
                android:layout_height="20dp"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="16dp"
                android:layout_marginBottom="16dp"
                android:background="@color/shimmer"
                android:textAppearance="?attr/textAppearanceCaption"
                app:layout_constraintBottom_toTopOf="@id/user_phone_title_shimmer"
                app:layout_constraintEnd_toEndOf="@id/vertical_guidline2"
                app:layout_constraintTop_toTopOf="parent" />

            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/user_name"
                android:layout_width="100dp"
                android:layout_height="20dp"
                android:layout_marginStart="16dp"
                android:background="@color/shimmer"
                android:textAppearance="?attr/textAppearanceSubtitle2"
                app:layout_constraintBaseline_toBaselineOf="@id/user_name_title_shimmer"
                app:layout_constraintStart_toEndOf="@id/vertical_guidline2" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/vertical_guidline2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent=".5" />

            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/user_phone_title_shimmer"
                android:layout_width="100dp"
                android:layout_height="20dp"
                android:layout_marginEnd="16dp"
                android:layout_marginBottom="16dp"
                android:background="@color/shimmer"
                android:textAppearance="?attr/textAppearanceCaption"
                app:layout_constraintBottom_toTopOf="@id/user_email_title_shimmer"
                app:layout_constraintEnd_toEndOf="@id/vertical_guidline2"
                app:layout_constraintTop_toBottomOf="@id/user_name_title_shimmer" />


            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/user_phone_shimmer"
                android:layout_width="100dp"
                android:layout_height="20dp"
                android:layout_marginStart="16dp"
                android:background="@color/shimmer"
                android:textAppearance="?attr/textAppearanceSubtitle2"
                app:layout_constraintBaseline_toBaselineOf="@id/user_phone_title_shimmer"
                app:layout_constraintStart_toEndOf="@id/vertical_guidline2" />

            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/user_email_title_shimmer"
                android:layout_width="100dp"
                android:layout_height="20dp"
                android:layout_marginEnd="16dp"
                android:layout_marginBottom="16dp"
                android:background="@color/shimmer"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@id/vertical_guidline2"
                app:layout_constraintTop_toBottomOf="@id/user_phone_title_shimmer" />

            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/user_email_shimmer"
                android:layout_width="100dp"
                android:layout_height="20dp"
                android:layout_marginStart="16dp"
                android:background="@color/shimmer"
                android:textAppearance="?attr/textAppearanceSubtitle2"
                app:layout_constraintBaseline_toBaselineOf="@id/user_email_title_shimmer"
                app:layout_constraintStart_toEndOf="@id/vertical_guidline2" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout65"
            style="@style/CardStyle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <com.google.android.material.textview.MaterialTextView
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:background="@color/shimmer"
                android:textAppearance="?attr/textAppearanceSubtitle2"
                android:textColor="?colorSecondary"
                app:layout_constraintBottom_toBottomOf="@id/imageView8"
                app:layout_constraintStart_toEndOf="@id/imageView8"
                app:layout_constraintTop_toTopOf="@id/imageView8" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/vertical_guidline342"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent=".5" />

            <LinearLayout
                android:layout_width="150dp"
                android:layout_height="24dp"
                android:layout_marginEnd="16dp"
                android:background="@color/shimmer"
                android:orientation="horizontal"
                app:layout_constraintBottom_toTopOf="@id/vertical_guidline342"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@id/vertical_guidline342" />


            <com.google.android.material.imageview.ShapeableImageView
                android:id="@+id/imageView8"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:layout_marginBottom="18dp"
                app:layout_constraintBottom_toBottomOf="@id/vertical_guidline342"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@id/vertical_guidline342"
                app:srcCompat="@drawable/ic_club_coin"
                app:tint="@color/shimmer" />

        </androidx.constraintlayout.widget.ConstraintLayout>


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout6"
            style="@style/CardStyle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/textView5"
                android:layout_width="100dp"
                android:layout_height="24dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:background="@color/shimmer"
                android:textAppearance="?attr/textAppearanceSubtitle1"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:orientation="horizontal"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/textView5">

                <androidx.coordinatorlayout.widget.CoordinatorLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/plane_08"
                    android:background="@android:color/transparent">

                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/badge"
                        style="@style/BadgeStyle"
                        android:layout_width="24dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="end"
                        android:layout_marginTop="55dp"
                        android:layout_marginEnd="10dp"
                        android:backgroundTint="@color/shimmer"
                        android:translationZ="10dp" />


                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center">

                        <com.google.android.material.textview.MaterialTextView
                            android:id="@+id/title"
                            android:layout_width="40dp"
                            android:layout_height="10dp"
                            android:layout_marginBottom="8dp"
                            android:background="@color/shimmer"
                            android:textAlignment="center"
                            android:textAppearance="?attr/textAppearanceOverline"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="@id/image"
                            app:layout_constraintStart_toStartOf="@id/image"
                            app:layout_constraintTop_toBottomOf="@id/image" />

                        <com.google.android.material.imageview.ShapeableImageView
                            android:id="@+id/image"
                            android:layout_width="60dp"
                            android:layout_height="60dp"
                            android:layout_margin="16dp"
                            android:background="@color/shimmer"
                            app:layout_constraintBottom_toTopOf="@id/title"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                </androidx.coordinatorlayout.widget.CoordinatorLayout>

                <androidx.coordinatorlayout.widget.CoordinatorLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/plane_08"
                    android:background="@android:color/transparent">

                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/badge22"
                        style="@style/BadgeStyle"
                        android:layout_width="24dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="end"
                        android:layout_marginTop="55dp"
                        android:layout_marginEnd="10dp"
                        android:backgroundTint="@color/shimmer"
                        android:translationZ="10dp" />


                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center">

                        <com.google.android.material.textview.MaterialTextView
                            android:id="@+id/title22"
                            android:layout_width="40dp"
                            android:layout_height="10dp"
                            android:layout_marginBottom="8dp"
                            android:background="@color/shimmer"
                            android:textAlignment="center"
                            android:textAppearance="?attr/textAppearanceOverline"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="@id/image22"
                            app:layout_constraintStart_toStartOf="@id/image22"
                            app:layout_constraintTop_toBottomOf="@id/image22" />

                        <com.google.android.material.imageview.ShapeableImageView
                            android:id="@+id/image22"
                            android:layout_width="60dp"
                            android:layout_height="60dp"
                            android:layout_margin="16dp"
                            android:background="@color/shimmer"
                            app:layout_constraintBottom_toTopOf="@id/title22"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                </androidx.coordinatorlayout.widget.CoordinatorLayout>

                <androidx.coordinatorlayout.widget.CoordinatorLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/plane_08"
                    android:background="@android:color/transparent">

                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/badge33"
                        style="@style/BadgeStyle"
                        android:layout_width="24dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="end"
                        android:layout_marginTop="55dp"
                        android:layout_marginEnd="10dp"
                        android:backgroundTint="@color/shimmer"
                        android:translationZ="10dp" />


                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center">

                        <com.google.android.material.textview.MaterialTextView
                            android:id="@+id/title33"
                            android:layout_width="40dp"
                            android:layout_height="10dp"
                            android:layout_marginBottom="8dp"
                            android:background="@color/shimmer"
                            android:textAlignment="center"
                            android:textAppearance="?attr/textAppearanceOverline"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="@id/image33"
                            app:layout_constraintStart_toStartOf="@id/image33"
                            app:layout_constraintTop_toBottomOf="@id/image33" />

                        <com.google.android.material.imageview.ShapeableImageView
                            android:id="@+id/image33"
                            android:layout_width="60dp"
                            android:layout_height="60dp"
                            android:layout_margin="16dp"
                            android:background="@color/shimmer"
                            app:layout_constraintBottom_toTopOf="@id/title33"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                </androidx.coordinatorlayout.widget.CoordinatorLayout>

                <androidx.coordinatorlayout.widget.CoordinatorLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/plane_08"
                    android:background="@android:color/transparent">

                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/badge44"
                        style="@style/BadgeStyle"
                        android:layout_width="24dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="end"
                        android:layout_marginTop="55dp"
                        android:layout_marginEnd="10dp"
                        android:backgroundTint="@color/shimmer"
                        android:translationZ="10dp" />


                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center">

                        <com.google.android.material.textview.MaterialTextView
                            android:id="@+id/title44"
                            android:layout_width="40dp"
                            android:layout_height="10dp"
                            android:layout_marginBottom="8dp"
                            android:background="@color/shimmer"
                            android:textAlignment="center"
                            android:textAppearance="?attr/textAppearanceOverline"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="@id/image44"
                            app:layout_constraintStart_toStartOf="@id/image44"
                            app:layout_constraintTop_toBottomOf="@id/image44" />

                        <com.google.android.material.imageview.ShapeableImageView
                            android:id="@+id/image44"
                            android:layout_width="60dp"
                            android:layout_height="60dp"
                            android:layout_margin="16dp"
                            android:background="@color/shimmer"
                            app:layout_constraintBottom_toTopOf="@id/title44"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                </androidx.coordinatorlayout.widget.CoordinatorLayout>

            </LinearLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <LinearLayout
            android:id="@+id/constraintLayout7"
            style="@style/CardStyle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="?colorSurface">


                <com.google.android.material.imageview.ShapeableImageView
                    android:id="@+id/icon"
                    android:layout_width="28dp"
                    android:layout_height="28dp"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="16dp"
                    android:layout_marginBottom="16dp"
                    android:background="@color/shimmer"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/text"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="16dp"
                    android:layout_marginEnd="16dp"
                    android:layout_marginBottom="16dp"
                    android:background="@color/shimmer"
                    android:textAppearance="?textAppearanceSubtitle2"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/imageView46"
                    app:layout_constraintStart_toEndOf="@id/icon"
                    app:layout_constraintTop_toTopOf="parent" />

                <com.google.android.material.imageview.ShapeableImageView
                    android:id="@+id/imageView46"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_alignParentEnd="true"
                    android:layout_marginTop="16dp"
                    android:layout_marginEnd="16dp"
                    android:layout_marginBottom="16dp"
                    android:background="@color/shimmer"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="?colorSurface">


                <com.google.android.material.imageview.ShapeableImageView
                    android:id="@+id/icon3"
                    android:layout_width="28dp"
                    android:layout_height="28dp"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="16dp"
                    android:layout_marginBottom="16dp"
                    android:background="@color/shimmer"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/text3"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="16dp"
                    android:layout_marginEnd="16dp"
                    android:layout_marginBottom="16dp"
                    android:background="@color/shimmer"
                    android:textAppearance="?textAppearanceSubtitle2"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/imageView4"
                    app:layout_constraintStart_toEndOf="@id/icon3"
                    app:layout_constraintTop_toTopOf="parent" />

                <com.google.android.material.imageview.ShapeableImageView
                    android:id="@+id/imageView4"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_alignParentEnd="true"
                    android:layout_marginTop="16dp"
                    android:layout_marginEnd="16dp"
                    android:layout_marginBottom="16dp"
                    android:background="@color/shimmer"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="?colorSurface">


                <com.google.android.material.imageview.ShapeableImageView
                    android:id="@+id/icon4"
                    android:layout_width="28dp"
                    android:layout_height="28dp"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="16dp"
                    android:layout_marginBottom="16dp"
                    android:background="@color/shimmer"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/text4"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="16dp"
                    android:layout_marginEnd="16dp"
                    android:layout_marginBottom="16dp"
                    android:background="@color/shimmer"
                    android:textAppearance="?textAppearanceSubtitle2"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/imageView5"
                    app:layout_constraintStart_toEndOf="@id/icon4"
                    app:layout_constraintTop_toTopOf="parent" />

                <com.google.android.material.imageview.ShapeableImageView
                    android:id="@+id/imageView5"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_alignParentEnd="true"
                    android:layout_marginTop="16dp"
                    android:layout_marginEnd="16dp"
                    android:layout_marginBottom="16dp"
                    android:background="@color/shimmer"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

            </androidx.constraintlayout.widget.ConstraintLayout>


        </LinearLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            style="@style/CardStyle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:orientation="vertical">

            <View
                android:layout_width="50dp"
                android:layout_height="24dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="16dp"
                android:layout_marginBottom="8dp"
                android:background="@color/shimmer"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <com.google.android.material.textview.MaterialTextView
                android:layout_width="70dp"
                android:layout_height="24dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="8dp"
                android:background="@color/shimmer"
                android:textAppearance="?textAppearanceSubtitle2"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </LinearLayout>
</layout>